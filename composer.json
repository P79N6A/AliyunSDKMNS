{
    "name": "shuguang/aliyun-sdk-mns",
    "description": "Aliyun MNS PHP SDK FOR COMPOSER",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "shuguang",
            "email": "ifsg@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "^6.3",
        "ext-openssl": "*",
        "ext-dom": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "AliyunMNS\\": "AliyunMNS/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AliyunMNS\\Tests\\": "tests/",
            "AliyunMNS\\Samples\\": "Samples/"
        }
    },"extra": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    },    
    "suggest": {
        "ext-openssl": "Allows working with https",
        "ext-curl": "To send requests using cURL",
        "ext-json": "Parse json"
    }
}
